<div class="chat-room-form">
 <h1>チャットルーム</div>
 <%= form_with model: @room, local: true do |f| %>
  <div class="chat-room-form-field">
   <%= f.label :チャットルーム名, class: 'chat-room-name' %>
  </div>
  <div>
   <%= f.text_field :name, class: 'chat__room_name ', placeholder: 'チャットルーム名を入力してください' %>
  </div>
  <div>
   <div class='chat-room-form__field--right'>
   <select>
    <option>ユーザーを選択してください</option>
    <option>ユーザー１</option>
    <option>ユーザー２</option>
   </select>
</div>
  </div>
  <%end%>
</div>
