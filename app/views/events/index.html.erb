<h1>イベント一覧</h1>
<%= link_to "企画する", new_event_path, class:"post" %>



<div class="contents">
  <% @events.each do |event| %>
    <div class="content_event">
      <p><%= event.name %></p>
        <span class="event_location">
          <%= event.location %>
        </span>
        <span class="event_genre">
          <%= event.genre_id %>
        </span>
        <span class="event_date">
          <%= event.date %>
        </span>
        <span class="event_description">
          <%= event.description %>
        </span>
    </div>
  <% end %>
</div>